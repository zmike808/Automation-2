<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="VSCOAndroid" preserve-order="true">
	<test name="SanityTest" preserve-order="true">
		<classes>
			<class name="tests.OnboardingTests">
				<methods>
					<include name="SignInProfileName"/>	
				</methods>
			</class>
			<class name="tests.ExploreTests" >
				<methods>
					<include name="ViewImageView" />
					<include name="FavoriteImage" />
					<include name="RepublishImage" />
					<include name="ViewCollectionUpdate" />
					<include name="ViewJournal" />
				</methods>
			</class>
			<class name="tests.ImportTests" >
				<methods>
					<include name="ImportImage" />
				</methods>
			</class>
			<class name="tests.CameraTests">
				<methods>
					<include name="BackCameraCapture"/>
					<include name="FrontCameraCapture"/>
				</methods>
			</class>
			<class name="tests.SignOutTests">
				<methods>
					<include name="SignOut"/>
				</methods>
			</class>
			<class name="tests.SettingsTests">
				<methods>
					<include name="VerifyUISignedOut" />
				</methods>
			</class>
			<class name="tests.SearchTests">
				<methods>
					<include name="PeopleImageJournalSearch" />
				</methods>
			</class>
		</classes>
	</test>
</suite>